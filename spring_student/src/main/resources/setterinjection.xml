    </bean>
    
    <bean id="id" class="com.xworkz.dp.Ids">
    <property name="panId" value="BPZ222338IP"></property>
    <property name="adhaarId" value="UiDPuN2022"></property>
    <property name="voterId" value="KAR92GJG"></property>    
    </bean>
    
    <bean id="me" class="com.xworkz.dp.Me"  autowire="byName" >
    <property name="id" value="211"></property>
    <property name="name" value="Loha"></property>
    <property name="listOfIds" ref="id"> </property>
    
