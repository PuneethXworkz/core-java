package com.xworkz.student;

public class Student {
	
	
	private int id;
	private String name;
	private String email;
	private String address;
	
	public Student() {
	}

	public Student(int id, String name, String email, String address) {
		super();
		this.id = id;
		this.name = name;
		this.email = email;
		this.address = address;
	}

	
	
	
}
