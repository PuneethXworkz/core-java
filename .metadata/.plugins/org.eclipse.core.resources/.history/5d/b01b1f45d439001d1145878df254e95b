package com.xworkz.team;

import com.xworkz.team.dto.PlayersDTO;
import com.xworkz.team.service.PlayersService;

public class PlayersDAO {

	public static void main(String[] args) {
		
		System.out.println("main method started");
		
		PlayersDTO player = new PlayersDTO();
		
		player.setName("");
		player.setState("Melbourn");
		player.setAge(38);
		player.setCountry("Australia");
		player.setSalary("96000");
		
		PlayersService service = new PlayersService();
		service.createPlayer(player);
		
		System.out.println("Main method ended");
	}

}
